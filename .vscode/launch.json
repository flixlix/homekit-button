  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://127.0.0.1:9123",
      "webRoot": "${workspaceFolder}/dist",
      "preLaunchTask": "debug",
      "postDebugTask": "devcontainer: stop debug",
      "skipFiles": ["**/frontend_latest/**"]
    }
  ]
}
